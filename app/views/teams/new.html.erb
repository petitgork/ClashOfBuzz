<h1>Creation d'une equipe</h1>
<div id="team-form">
  <%= simple_form_for @team do |f| %>
    <%= f.input :name, label: "Nom de l'équipe" %>
    <%= f.input :number_of_politics, label: "nombre de politics" %>
    <%= f.input :avatar, label: "Avatar", as: :file %>
    <%= f.button :submit, "Je suis prêt" %>
  <% end %>
</div>
